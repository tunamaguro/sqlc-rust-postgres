{
  "version": "2",
  "plugins": [
    {
      "name": "rust-postgres",
      "wasm": {
        "url": "file://target/wasm32-wasip1/release/sqlc-rust-postgres.wasm",
        "sha256": "$WASM_SHA256"
      }
    }
  ],
  "sql": [
    {
      "schema": "schema.sql",
      "queries": "query.sql",
      "engine": "postgresql",
      "codegen": [
        {
          "out": "gen",
          "plugin": "rust-postgres"
        }
      ]
    }
  ]
}